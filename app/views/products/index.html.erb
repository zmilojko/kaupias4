<h1 id="b">List of all products</h1>

<table class="oscar" style="background-color:#00f;">
  <tr>
    <th onclick="javascript:myproc()">ID</th>
    <th>Product name</th>
    <th>Quantity</th>
    <th>Weight</th>
    <th>Tax class</th>
    <th>Status</th>
  </tr>

<% @products.each do |product| %>
  <tr>
    <td><%= product.id %></td>
    <td><%= product.name %></td>
    <td><%= product.products_quantity %></td>
    <td><%= product.products_weight %></td>
    <td><%= product.products_tax_class_id %></td>
    <td><%= product.products_status %></td>
  </tr>
<% end %>
</table>

<br />

